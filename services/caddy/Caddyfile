# Plex reverse proxy - not needed but sometimes useful.
https://plex.prickels.casa {
  tls mike@mikeyp.net
  proxy / http://192.168.60.203/ {
    transparent
  }
}

# Reverse proxy to Dream Machine setup - one again not required, but useful if ui.com proxy is slow.
https://unifi.prickels.casa {
  tls mike@mikeyp.net
  proxy / https://setup.ui.com/ {
    websocket
    transparent
    insecure_skip_verify
  }
}

# Homeassistant
https://home.prickels.casa {
  tls mike@mikeyp.net
  proxy / http://home.inside.prickels.casa:8123/ {
    websocket
    transparent
  }
}

